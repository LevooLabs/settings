{
    "name": "levoolabs/settings",
    "description": "Settings table for Laravel.",
    "keywords": ["settings", "cache", "eloquent", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Adam Troll",
            "email": "adam.troll@levoolabs.com"
        },
        {
            "name": "Peter Cserni",
            "email": "peter.cserni@levoolabs.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.1.3",
        "illuminate/database": "^5.4",
        "illuminate/support": "^5.4",
        "illuminate/cache": "^5.4"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "LevooLabs\\Settings\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LevooLabs\\Settings\\SettingsServiceProvider"
            ],
            "aliases": {
                "Setting": "LevooLabs\\Settings\\Facades\\Setting"
            }
        }
    }
}
